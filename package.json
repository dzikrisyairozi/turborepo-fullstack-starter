{
  "name": "turborepo-fullstack-starter",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "lint:fix": "turbo run lint:fix",
    "lint:check": "turbo run lint:check",
    "format": "prettier --write \"**/*.{ts,tsx,md,json,js,jsx,css,scss,yaml,yml}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,md,json,js,jsx,css,scss,yaml,yml}\"",
    "type-check": "turbo run type-check",
    "check-types": "turbo run check-types",
    "test": "turbo run test",
    "test:watch": "turbo run test:watch",
    "clean": "turbo run clean",
    "dev:web": "turbo run dev --filter=web",
    "dev:api": "turbo run dev --filter=api",
    "dev:docs": "turbo run dev --filter=docs",
    "build:web": "turbo run build --filter=web",
    "build:api": "turbo run build --filter=api",
    "build:docs": "turbo run build --filter=docs",
    "db:generate": "turbo run db:generate --filter=api",
    "db:push": "turbo run db:push --filter=api",
    "db:migrate": "turbo run db:migrate --filter=api",
    "db:studio": "turbo run db:studio --filter=api",
    "prepare": "husky install",
    "commitlint": "commitlint --edit",
    "check-all": "pnpm run type-check && pnpm run lint && pnpm run format:check"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/compat": "^1.3.2",
    "@eslint/eslintrc": "^3.2.0",
    "@types/node": "^24.3.0",
    "es-abstract": "^1.24.0",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.5",
    "prettier": "^3.6.2",
    "turbo": "^2.5.6",
    "typescript": "5.9.2",
    "typescript-eslint": "^8.39.1"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18"
  }
}
